{
  // Supresses warnings in a frontmatter html comment. This is useful
  // because of SPDX-FileCopyrightText going over the line length.
  // This regexp only works properly if the markdown starts with a heading
  // after the initial comment, which it should, and the character # is
  // not used in the heading, which it shouldn't.
  "frontMatter": "<!--[^#]*-->(\\r\\n|\\r|\\n|$)"
}
